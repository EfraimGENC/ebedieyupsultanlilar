{
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "tailwindCSS.classAttributes": ["class", "ui"],
  "tailwindCSS.experimental.classRegex": [
    ["ui:\\s*{([^)]*)\\s*}", "(?:'|\"|`)([^']*)(?:'|\"|`)"]
  ],
  "i18n-ally.localesPaths": ["i18n/locales", "i18n"],
  "i18n-ally.keystyle": "nested",
  "[markdown]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true
  }
}
